    "got": "^11.8.1",
    "http-errors": "^1.8.0",
    "joi": "^17.3.0",
    "morgan": "~1.9.1",
    "pg": "^8.5.1",
    "pg-pool": "^3.2.2",
    "pino": "^6.11.0",
    "pino-pretty": "^4.3.0",
